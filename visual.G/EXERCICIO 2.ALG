Algoritmo "exercicio"

Var
nota1,nota2,op,soma:real

Inicio

escreval ("entre com um numero1")
leia (nota1)
escreval ("entre com um numero2")
leia (nota2)
escreval ("entre com op soma")
leia (op)
se (op<=9) então
      soma <- nota1 + nota2
      escreval ("A soma é", soma)
  senao
      escreval("op invalido")
fimse
Fimalgoritmo